<html>
    <body>
        <h1>Cadastro de Produto</h1>
        <form action="/cadastro/produto/alva"method="post">
            <label>Nome:</label>
            <input name="nome" type="text"/><br/>
            <label>Quantidade</label>
            <input name="Quantidade" type="text"/><br/>            
            <label>Preço</label>
            <input name="preço" type="text"/><br/>
            <input type="submit" value="cadastrar"/>
        </form>
        <% if(mensagem){%>
            <p>
                <label style="color: red;"><%=mensagem %></label>
            </p>
        <%}%>
    </body>
</html>